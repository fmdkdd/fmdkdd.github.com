---
layout: nil
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>fmdkdd</title>
  <link href="http://fmdkdd.github.com/atom.xml" rel="self"/>
  <link href="http://fmdkdd.github.com/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://fmdkdd.github.com/</id>
  <author>
	 <name>Florent Marchand de Kerchove de Denterghem</name>
	 <email>fmdkdd@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
	 <title>{{ post.title }}</title>
	 <link href="http://fmdkdd.github.com{{ post.url }}"/>
	 <updated>{{ post.date | date_to_xmlschema }}</updated>
	 <id>http://fmdkdd.github.com{{ post.id }}</id>
	 <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}

</feed>
